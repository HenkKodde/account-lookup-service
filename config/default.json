{
  "PORT": 4002,
  "DATABASE_URI": "mysql://account_lookup:password@localhost:3306/account_lookup",
  "RUN_MIGRATIONS": true,
  "ENDPOINT_SOURCE_URL": "http://localhost:3001/participants/{{fsp}}/endpoints",
  "ENDPOINT_CACHE_CONFIG": {
    "expiresIn": 180000,
    "generateTimeout": 30000
  },
  "PARTICIPANT_SOURCE_URL": "http://localhost:3001/participants/{{fsp}}",
  "INSTRUMENTATION": {
    "METRICS": {
      "DISABLED": false,
      "labels": {
        "fspId": "*"
      },
      "config": {
        "timeout": 5000,
        "prefix": "moja_als_",
        "defaultLabels": {
          "serviceName": "account-lookup-service"
        }
      }
    }
  }
}
